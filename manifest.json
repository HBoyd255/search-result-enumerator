{
    "manifest_version": 2,
    "name": "Search Result Enumerator",
    "version": "1.0",
    "description": "Adds numbers to Google search results, allowing you to navigate them by pressing the corresponding key.",
    "permissions": [
        "activeTab",
        "https://www.google.com/"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.google.com/search*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}