{
    "manifest_version": 2,
    "name": "Search Result Quick Navigator",
    "version": "1.2.2",
    "description": "Adds numbers to Google search results, and letters to the different search tabs, allowing you to navigate them by pressing the corresponding key.",
    "icons": {
        "128": "icons/logo128.png",
        "96": "icons/logo96.png",
        "64": "icons/logo64.png",
        "48": "icons/logo48.png",
        "32": "icons/logo32.png",
        "16": "icons/logo16.png"

    },
    "content_scripts": [
        {
            "matches": [
                "*://*.google.com/search*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}